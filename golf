using namespace std;

void ReadFile(string filename){
    ifstream inputfile(filename);
    if (not inputfile) {
        cout << "Error: THe specified file cannot be opened!"<<endl;
    }

}

int main()
{
    string filename;
    cout<<"File input: "
    cin>> filename;
    ReadFile(filename);
}
